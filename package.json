{
  "name": "nest-monorepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "start:gateway": "npm --workspace apps/gateway run start:dev",
    "start:auth": "npm --workspace apps/authentication run start:dev",
    "start:all": "concurrently \"npm: start:gateway\" \"npm: start:auth\""
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.10.0",
    "@types/passport-jwt": "^4.0.1",
    "concurrently": "^9.2.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.8",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "^6.18.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "luxon": "^3.7.2",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  }
}
